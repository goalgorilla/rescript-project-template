{
  "name": "@open-social/rescript-react-template",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:rescript": "rescript build -w",
    "dev:esbuild": "NODE_ENV=development node ./dev.js",
    "dev:server": "NODE_ENV=development serve public/",
    "build": "run-s build:*",
    "build:rescript": "rescript",
    "build:esbuild": "node ./build.js",
    "start": "serve public/",
    "clean": "rescript clean && rm public/bundle.js"
  },
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rescript": "^9.1"
  },
  "devDependencies": {
    "@rescript/react": "^0.10.3",
    "esbuild": "^0.14.29",
    "npm-run-all": "^4.1.5",
    "serve": "^13.0.2"
  }
}
